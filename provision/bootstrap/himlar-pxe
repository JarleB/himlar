  <name>himlar-pxe</name>
  <forward mode='nat'>
    <nat>
      <port start='1024' end='65535'/>
    </nat>
  </forward>
  <bridge name='hbr0' stp='on' delay='0'/>
  <ip address='10.0.3.1' netmask='255.255.255.0'>
    <tftp root='/var/lib/tftpboot'/>
    <dhcp>
      <range start='10.0.3.200' end='10.0.3.254'/>
      <bootp file='pxelinux.0'/>
    </dhcp>
  </ip>
</network>

